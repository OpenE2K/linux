OUTPUT_FORMAT("elf64-e2k", "elf64-e2k", "elf64-e2k")
/* OUTPUT_ARCH(e2k) Defined by Makefile */
ENTRY(start)

SECTIONS {
	_start = .;
	.bootblock : {
		_bootblock = .;
			arch/e2k/boot/bootblock.o(.rodata)
		_ebootblock = .;
	}

	. = _bootblock + 0x10000;

	.loader : {
		. += 0x6000;
		*(.boot_entry)
		*(.text)
		*(EXCLUDE_FILE (arch/e2k/boot/cpiggy.o) .rodata)
		*(.data)

		_bss = .;
		*(.bss)
		_ebss = .;
	}
	.kernel : {
		_kernel = .;
		arch/e2k/boot/cpiggy.o(.rodata)
		_ekernel = .;
	}
	_end = .;
}
